# Browser-exposed (prefix VITE_)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
VITE_APP_NAME=Docket Chief
VITE_ENVIRONMENT=development
VITE_PAYMENTS_ENABLED=false

# Server-only (Edge Functions / backend)
OPENAI_API_KEY=

# CORS Configuration - Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:8080,https://docketchief.com,https://www.docketchief.com

# Security Configuration
# Enable rate limiting (recommended: true)
ENABLE_RATE_LIMITING=true

